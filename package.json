{
  "name": "materia-widget-development-kit",
  "description": "Materia Widget Development Kit (MWDK) is a local dev environment for quickly building Materia Widgets.",
  "license": "AGPL-3.0",
  "homepage": "https://ucfopen.github.io/Materia-Docs/",
  "version": "2.4.0-alpha1",
  "author": "University of Central Florida, Center for Distributed Learning",
  "scripts": {
    "build": "webpack",
    "test": "echo 'No tests yet'",
    "start": "env TEST_MDK=true webpack-dev-server --open --config webpack.config.widget.js",
    "postinstall": "yarn build",
    "prettier:run": "prettier --write 'src/*.js' --write 'assets/**/*.{js,css}' --write '*.js'",
    "clientAssets:link": "cd ../materia-server-client-assets && yarn link && cd - && yarn link materia-server-client-assets"
  },
  "materia": {
    "cleanName": "MWDK"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "angular-mocks": "1.6.9",
    "autoprefixer": "^7.1.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^2.0.2",
    "coffee-loader": "^0.7.3",
    "coffeescript": "^2.4.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.16.0",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "hogan-express": "^0.5.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.29.0",
    "materia-server-client-assets": "^2.0.0-alpha1",
    "node-sass": "^4.12.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.11.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.20.2",
    "wait-until-promise": "^1.0.0",
    "webpack": "3.12.0",
    "webpack-dev-server": "^2.11.3",
    "webpack-strip-block": "^0.2.0",
    "whatwg-fetch": "^3.0.0",
    "yamljs": "^0.2.8",
    "zip-webpack-plugin": "^2.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "useTabs": true,
    "singleQuote": true
  }
}
